<h1>Songs</h1>

<br></br>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= link_to "Delete", song, method: :delete, data: {
            confirm: "For real?"
          } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
